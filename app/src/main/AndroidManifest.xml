<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.hannah.mybookingapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.views.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.views.AuthActivity">

        </activity>
        <activity
            android:name=".ui.views.AddListings"
            android:label="Add Listing" />
        <activity
            android:name=".ui.views.BookingsAvailable"
            android:label="Bookings Available" />
        <activity android:name=".ui.views.MainActivity">

        </activity>
    </application>

</manifest>